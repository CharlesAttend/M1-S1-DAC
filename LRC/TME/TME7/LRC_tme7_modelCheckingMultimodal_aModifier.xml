            <parameter>w</parameter>
            <parameter>isItTrue pos variable R variable A</parameter>
        </condition>
        <condition>
            <condition-name>isLinked</condition-name>
            <parameter>w</parameter>
            <parameter>u</parameter>
            <parameter>variable R</parameter>
        </condition>
